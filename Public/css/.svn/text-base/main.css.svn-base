.content h2{
	border-bottom: none;
	margin-top: 1em;
	margin-bottom: 0.5em;
	color: #444;
}
#job_submit {
	font-size: 0.8em;
	background: #D6E9F8;
	padding: 5px;
	float: right;
	margin-right: 15px;
}
#job_submit span.submit input{
	width:100%;
	height: 50px;
	width: 100%;
	font-size: 20px;
	font-weight: bold;
}

#online_quality_delta,#offline_quality_profit{
	float: left;
	width: 30%;
}

#online_quality_delta table,#offline_quality_profit table{
	width: 80%;
	line-height: 30px;
	font-family: Microsoft Yahei;
	color: #8A8989;
}

#offline_quality_profit{
	margin-left:8%;
	padding-left: 15px;
	border-left: 1px dotted #CCC;
}

#today_status .tq_satus{
	float: left;
	width: 15%;
	border-left: 1px dotted #CCC;
//	margin: 0px 15px;
	padding: 0px 2%;
}
#today_status .tq_satus:first-child{
	border-left: none;
}
#quality_today .tq_satus span{
	font-size: 26px;
	color: #8A8989;
	font-weight: bold;
	padding-bottom: .25em;
	margin-bottom: 1em;
	font-family: Microsoft Yahei;
}
#quality_today .tq_satus h4{
	font-size: 12px;
	font-weight: bold;
	color: #888;
	margin-bottom: 1.5em;
}
